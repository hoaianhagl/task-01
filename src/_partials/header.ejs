<header class="c-header">
  <a class="c-header__logo" href="<%= BASE_URL %>index.html"><img src="../assets/img/img.png" alt="Logo"></a>
  <nav class="c-gnav">
    <ul>
      <% if (typeof linklists != 'undefined') { %>
        <% linklists.main && linklists.main.forEach(link => { %>
          
            <li><a <% if (link.current || link.active) { %>class="is-active"<% } %> href="<%= link.url %>"><%= link.title %></a></li>
            <% if (link.links) { %>
                <% link.links.forEach(child_link => { %>
                  <li>
                    <a <% if (child_link.current) { %>class="is-active"<% } %> href="<%= child_link.url %>"><%= child_link.title %></a>
                  </li>
                <% }) %>
            <% } %>
          
        <% }) %>
      <% } %>
    </ul>
  </nav>
</header>
